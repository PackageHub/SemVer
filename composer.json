{
    "name": "package-hub/semver",
    "description": "Package to check version according to Semantic Versioning 2.0.0 (http://semver.org).",
    "authors": [{"name": "Gordon Schmidt", "email": "schmidt.gordon@web.de"}],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/PackageHub/SemVer",
    "keywords": ["php", "semver"],
    "autoload": {
        "psr-0": {
            "PhSemVer": "src/",
            "PhSemVerTest": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "phpmd/phpmd": "~1.5",
        "squizlabs/php_codesniffer": "~1.5",
        "satooshi/php-coveralls": "~0.6"
    }
}
